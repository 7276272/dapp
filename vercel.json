{
  "buildCommand": "yarn g:build",
  "outputDirectory": "apps/web/build",
  "installCommand": "yarn install",
  "framework": null,

  "env": {
    "REACT_APP_INFURA_KEY": "c167d55c2abb4c0fad5dba3107333036",
    "REACT_APP_ALCHEMY_API_KEY": "1lhaATkVANvh60-cB54xDlpUd1p3e3sk",
    "REACT_APP_WALLET_CONNECT_PROJECT_ID": "2fac74160793b250ca8768f5328fd8e9",
    "INCLUDE_PROTOTYPE_FEATURES": "true",
    "NODE_OPTIONS": "--max-old-space-size=8192"
  },

  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, s-maxage=60, stale-while-revalidate=30"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/favicon.ico",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
